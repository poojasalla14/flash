<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	
	<context-param>
		<param-name>copyrights</param-name>
		<param-value>pushpa</param-value>
	</context-param>
	
	<servlet>
		<servlet-name>GenericExample</servlet-name>
		<servlet-class>com.training.servletEx.TestServletConfig</servlet-class>
		<init-param>
			<param-name>author</param-name>
			<param-value>pushpa</param-value>
		</init-param>
	</servlet>
	
	<servlet>
		<servlet-name>ColorExample</servlet-name>
		<servlet-class>com.training.servletEx.ColorGetServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.training.servletEx.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SecondServlet</servlet-name>
		<display-name>SecondServlet</display-name>
		<description></description>
		<servlet-class>com.training.servletEx.SecondServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>GenericExample</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ColorExample</servlet-name>
		<url-pattern>/colors</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SecondServlet</servlet-name>
		<url-pattern>/SecondServlet</url-pattern>
	</servlet-mapping>
	
</web-app>
